<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <ReleaseVersion>0.1.0</ReleaseVersion>
    <RootNamespace>webdavnet</RootNamespace>
    <Description>WebDAVNet is a client library for interacting with WebDAV written in C#.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="WebClientCore" />
    <None Remove="System.Net.Http" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\webclient\WebClientCore\WebClientCore.csproj" />
  </ItemGroup>
</Project>
